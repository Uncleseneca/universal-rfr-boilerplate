{
  "name": "react-redux-ssr-boilerplate",
  "description": "Boilerplate project",
  "version": "1.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "scripts": {
    "flow": "flow",
    "start":
      "npm run clean && cross-env NODE_ENV=development babel-watch --inspect server/index.js",
    "start:prod": "npm run build && npm run serve",
    "serve": "cross-env NODE_ENV=production node buildServer/index.js",
    "build":
      "npm run build:client && npm run build:server && npm run build:node",
    "build:client":
      "rimraf buildClient && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server":
      "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node":
      "cross-env NODE_ENV=production babel server/index.js -o buildServer/index.js",
    "clean": "rimraf buildClient buildServer",
    "format":
      "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && npm run lint",
    "svgr": "svgr -d icons/out icons/raw"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-universal-import": "^1.4.0",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.5.1",
    "css-modules-require-hook": "^4.2.3",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "express": "^4.15.2",
    "file-loader": "^1.1.11",
    "google-map-react": "^0.34.0",
    "history": "^4.6.3",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "points-cluster": "^0.1.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-inline-svg": "^3.0.0",
    "postcss-loader": "^2.1.0",
    "query-string": "^5.1.0",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-key-handler": "^1.0.1",
    "react-redux": "^5.0.5",
    "react-sticky": "^6.0.2",
    "react-stickynode": "^1.4.1",
    "react-tippy": "^1.2.2",
    "react-transition-group": "^2.3.0",
    "react-truncate-markup": "^1.0.1",
    "react-universal-component": "2.8.4",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^0.0.20-next",
    "redux-first-router-link": "^1.1.4",
    "redux-thunk": "^2.2.0",
    "redux-windowsize": "^1.3.1",
    "reselect": "^3.0.1",
    "serve-favicon": "^2.4.5",
    "styled-components": "^3.2.3",
    "svg-url-loader": "^2.3.2",
    "svgr": "^1.9.1",
    "webpack-flush-chunks": "1.2.3"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "babel-watch": "^2.0.6",
    "concurrently": "^3.5.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-css-chunks-webpack-plugin": "^2.0.15",
    "flow-bin": "^0.71.0",
    "prettier": "^1.4.4",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.6.1",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.2.0",
    "write-file-webpack-plugin": "^4.1.0"
  }
}
